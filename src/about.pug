extends base/pug/base
//- include ./components/slider/slider
//- include ./components/links/links

append variables
    - var title = 'Главная страница сайта'
    - var pageClass = 'homepage'
    - var menuHref = '/'

block about 
    .outer
        .inner(style="padding-top: 27px; border-top: 1px solid #584C3B;")
            nav.breadcrumbs
                ul.breadcrumbs__list
                    li
                        a(href="/") Главная
                        span.breadcrumbs__divider \
                    li.is-active #{title}
    section.about 
        .outer 
            .inner 
                h1.title Компания Ваша Терраса
                .about__container 
                    .about__content
                        p.t20.w400 За 8 лет строительства террас мы на практике изучили свойства и характеристики террасных досок 
                        p.t18.w300 Компания "ВАША ТЕРРАСА" была основана 2014 году, и вот уже более 5 лет наш коллектив профессиональных строителей и проектировщиков создает для Вас долговечные и красивые террасы, заборы и ограждения, мостики и дорожки, помогает облицовывать фасады загородных домов и обустраивает общественные парки, кафе, рестораны, причалы и пристани.
                        p.t18.w300 
                            span.w500 Мы с одинаковым профессионализмом 
                            | и вниманием относимся, как к небольшим частным объектам, так и к крупным заказчикам - юридическим лицам.
                    a(href="#").video
                        img(src="./images/about/about.jpg" alt="Видео")
                ul.about__achievements
                    li 
                        p 2014
                        span компания основная в 2014 году
                    li 
                        p 9879+
                        span довольных клентов своей новой террасой
                    li 
                        p 787 500м<sup>2</sup>
                        span профессионально смотнтированных террас
block bogach 
    section.bogach 
        .outer 
            .inner 
                .bogach__container 
                    .bogach__author 
                        .bogach__img
                            img(src='./images/bogach/photo.jpg' alt="Александр Богач")
                        p Александр Богач, компания 
                            span “Ваша Терраса”
                    .bogach__content 
                        h2.title Мы уважаем и любим своих клиентов
                        p.t20.w400 Все наше совместное общение в процессе сотрудничества и даже после него (что, немаловажно), будет строиться исключительно в позитивном и уважительном ключе.
                        p.t18.w300 Наша задача не просто построить объект, а помочь вам реализовать все идеи и задумки так, чтобы созданная терраса дарила радость и комфорт долгие годы.
